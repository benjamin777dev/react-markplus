{
  "name": "markdown-plus",
  "version": "2.5.2",
  "description": "Markdown editor with extra features",
  "keywords": [
    "markdown",
    "editor",
    "markdown plus",
    "mdp"
  ],
  "homepage": "https://github.com/tylerlong/markdown-plus/",
  "bugs": {
    "url": "https://github.com/tylerlong/markdown-plus/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tylerlong/markdown-plus.git"
  },
  "license": "MIT",
  "author": "Tyler Liu",
  "main": "index.js",
  "scripts": {
    "build": "tsx node_modules/.bin/webpack --mode production",
    "lint": "eslint . --fix && prettier --write . && sort-package-json",
    "serve": "tsx node_modules/.bin/webpack serve"
  },
  "dependencies": {
    "chart.js": "2.6.0",
    "codemirror": "5.26.0",
    "jquery": "2.2.4",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "markdown-core": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "css-loader": "^7.1.2",
    "eslint-config-tyler": "^0.1.4",
    "globals": "^15.10.0",
    "html-webpack-plugin": "^5.6.0",
    "mini-css-extract-plugin": "^2.9.1",
    "path-browserify": "^1.0.1",
    "sort-package-json": "^2.10.1",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "yarn-upgrade-all": "^0.7.4"
  },
  "yarn-upgrade-all": {
    "ignore": [
      "jquery",
      "markdown-core",
      "chart.js",
      "codemirror"
    ]
  }
}
